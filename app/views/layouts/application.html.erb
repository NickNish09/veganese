<!DOCTYPE html>
<html>
  <head>
    <title>Veganese</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <%= stylesheet_link_tag "vendor/bootstrap/css/bootstrap.min.css" %>
    <%= stylesheet_link_tag "vendor/fontawesome-free/css/all.min.css" %>
    <%= stylesheet_link_tag "https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,700,300italic,400italic,700italic" %>
    <%= stylesheet_link_tag "vendor/simple-line-icons/css/simple-line-icons.css" %>
    <%= stylesheet_link_tag "css/stylish-portfolio.css" %>
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@8"></script>
    <link href="https://fonts.googleapis.com/css?family=Indie+Flower" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">

    <%= favicon_link_tag "img/favicon.ico"%>
  </head>

  <body>
    <%= yield %>
    <%#= javascript_include_tag "vendor/jquery/jquery.min.js" %>
    <%= javascript_include_tag "vendor/bootstrap/js/bootstrap.bundle.min.js" %>
    <%= javascript_include_tag "vendor/jquery-easing/jquery.easing.min.js" %>
    <%= javascript_include_tag "js/stylish-portfolio.min.js" %>
  </body>
</html>
